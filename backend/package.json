    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "pino": "^6.13.0",
    "qs": "^6.10.1",
    "stripe": "^8.167.0",
    "tinyduration": "^3.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@types/aws-lambda": "^8.10.81",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/jwt-decode": "^3.1.0",
