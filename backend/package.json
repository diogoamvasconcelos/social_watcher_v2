    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-mock-axios": "^4.3.0",
    "monocle-ts": "^2.3.10",
    "newtype-ts": "^0.3.4",
